{
  "name": "guava3shome-h5-utils",
  "version": "1.4.2",
  "description": "",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup lib --format cjs,esm --dts --clean --sourcemap",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "guava3shome",
    "utils"
  ],
  "author": "HuangSouth <2168626265@qq.com>",
  "devDependencies": {
    "@types/node": "^22.10.10",
    "typescript": "^5.7.3",
    "tsup": "^8.5.1"
  }
}
